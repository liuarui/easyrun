    "@types/inquirer": "^7.3.1",
    "chalk": "^4.1.1",
    "eslint": "^7.26.0",
    "inquirer": "^8.0.0",
    "ora": "^5.4.0",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "commander": "^7.2.0"
  },
  "publishConfig": {
