  "description": "Let your script command run more simple!",
  "author": "liuarui <liuwenrui597@qq.com>",
  "license": "MIT",
  "keywords": [
    "easyrun",
    "scripts",
    "npm",
    "yarn",
    "run scrips",
    "utils",
    "helper"
  ],
  "bin": {
    "erun": "./bin/erun.js"
  },
